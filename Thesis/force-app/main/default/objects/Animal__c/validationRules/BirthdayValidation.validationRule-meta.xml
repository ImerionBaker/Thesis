<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BirthdayValidation</fullName>
    <active>true</active>
    <errorConditionFormula>Birthdate__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Birthdate__c</errorDisplayField>
    <errorMessage>Birthday can&apos;t be in future</errorMessage>
</ValidationRule>
