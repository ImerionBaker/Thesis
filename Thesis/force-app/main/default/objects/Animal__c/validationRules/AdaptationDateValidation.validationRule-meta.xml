<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AdaptationDateValidation</fullName>
    <active>true</active>
    <errorConditionFormula>AdoptationStartDate__c &gt; AdoptationalEndDate__c</errorConditionFormula>
    <errorMessage>The date of the planned end of adaptation cannot be earlier than the start date of adaptation</errorMessage>
</ValidationRule>
